  "scripts": {
    "dev": "NODE_ENV=development tsx --watch server/index.ts",
    "build": "npm run build:client && npm run build:express",
    "build:client": "vite build",
    "build:express": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push && npm run restart-express",
    "lint": "eslint .",
    "preview": "vite preview",
    "restart-express": "touch server/index.ts",
    "cz": "git-cz"
  },
